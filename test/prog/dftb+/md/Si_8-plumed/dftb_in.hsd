  SCC = Yes
  SCCTolerance = 1e-10 # extremly tight, only for test purposes
  MaxAngularMomentum = {
    Si = "p"
  }
  Charge = 0.000000000000000E+000
  SpinPolarisation = {}
  Eigensolver = Standard {}
  Filling = Fermi {
    Temperature [Kelvin] = 116.045
  }
  SlaterKosterFiles = {
    Si-Si = "./Si-Si.skf"
  }
