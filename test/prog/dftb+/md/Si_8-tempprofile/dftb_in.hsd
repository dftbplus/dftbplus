Driver = VelocityVerlet {  
  TimeStep [Femtosecond] = 1.00000000000000
  Thermostat = Andersen {
    Temperature [Kelvin] = TemperatureProfile {
      constant      1    700.0
      linear       50   1300.0
      constant    150   1300.0
      exponential  50     20.0
    }
    ReselectProbability = 0.200000000000000
    ReselectIndividually = No
    AdaptFillingTemp = Yes
  }
  OutputPrefix = "geo_end"
}

