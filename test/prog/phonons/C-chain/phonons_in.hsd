geometry = genformat {
<<< "c24.gen"
}

transport {
    device {
      atomrange = 1 8 
    }
    contact {
      Id = "Source"
      PLShiftTolerance = 1e-4
      atomrange = 9 16
      temperature [K] = 100.0
    }
    contact {
      Id = "Drain"
      PLShiftTolerance = 1e-4
      atomrange = 17 24
      temperature [K] = 200.0
    }
}

Masses = {}

Hessian = {
  #Cutoff = 1e-1
  Matrix = dftb 
}

Analysis {
  TunnelingAndDOS{
    Verbosity = 91
    FreqRange [eV] = 1e-6 4e-1
    FreqStep [eV] = 5e-3
    Delta = 1e-4
  }
}

#options = {
#  writeautotesttag = yes 
#}

parseroptions = {
  WriteHSDInput = Yes 
  WriteXMLInput = No
  parserversion = 4
}

